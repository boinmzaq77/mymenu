                            <hr />
                        </div>
                        <div class="col-12 px-0">
                            <p class="mb-2 text-head-p">@Localizer["กลุ่มสินค้าตามจุดพิมพ์ :"]</p>
                        </div>
                        <div class="col-12 px-0">
